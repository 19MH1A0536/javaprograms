//Write a Java Program to display default value of all primitive data types of Java.

class P1536
{
 static byte b;
 static short s;
 static int i;
 static long l;
 static float f;
 static double d;
 static char c;
 static boolean bl;
 
 public static void main(String args[])
 {
  System.out.println("Default value of byte:"+b);
  System.out.println("Default value of short:"+s);
  System.out.println("Default value of integer:"+i);
  System.out.println("Default value of long:"+l);
  System.out.println("Default value of float:"+f);
  System.out.println("Default value of double:"+d);
  System.out.println("Default value of character:"+c);
  System.out.println("Default value of boolean:"+bl);
 }
} 
 
